{
  "name": "screen-recorder",
  "main": "./lib/screen-recorder",
  "version": "1.2.3",
  "description": "Record your atom editor into a gif",
  "keywords": [
    "screen",
    "recording",
    "recorder",
    "gif"
  ],
  "activationCommands": {
    "atom-workspace": [
      "screen-recorder:open-select-area",
      "screen-recorder:record-window",
      "screen-recorder:record-tree-view",
      "screen-recorder:record-active-pane"
    ]
  },
  "repository": "https://github.com/lexcast/screen-recorder",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "atom-utils": "^0.9.2",
    "fluent-ffmpeg": "^2.0.1",
    "fs-plus": "^2.8.1",
    "imagemagick": "^0.1.3",
    "path": "^0.12.7"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
